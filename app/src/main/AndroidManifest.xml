<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notes_applicationn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_notes_foreground"
        android:label="Notes App"
        android:roundIcon="@mipmap/ic_notes_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.notes_applicationn">
        <activity android:name="com.example.notes_applicationn.AddNoteActivity"></activity>
        <activity android:name="com.example.notes_applicationn.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>